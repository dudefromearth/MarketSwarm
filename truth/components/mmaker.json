{
  "id": "mmaker",
  "meta": {
    "name": "Redis model maker",
    "description": "Consumes Massive chain snapshots + websocket diffs and builds convexity and regime models for the UI and Vexy."
  },
  "access_points": {
    "publish_to": [
      { "bus": "system-redis", "key": "mmaker:heartbeat" }
    ],
    "subscribe_to": [
      { "bus": "market-redis", "key": "massive:chain-feed" },
      { "bus": "market-redis", "key": "massive:ws:raw" }
    ]
  },
  "models": {
    "produces": [
      { "bus": "market-redis", "key": "massive:model:gex" },
      { "bus": "market-redis", "key": "massive:model:heatmap" },
      { "bus": "market-redis", "key": "massive:model:market_mode" },
      { "bus": "market-redis", "key": "massive:model:vix_regime" },
      { "bus": "market-redis", "key": "massive:model:volume_profile" },
      { "bus": "market-redis", "key": "massive:model:bias_lfi" }
    ],
    "consumes": [
      { "bus": "market-redis", "key": "massive:model:spot" }
    ]
  },
  "heartbeat": {
    "interval_sec": 5,
    "ttl_sec": 15
  },
  "dependencies": [
    "massive",
    "vexy_ai"
  ]
}