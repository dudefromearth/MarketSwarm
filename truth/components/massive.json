{
  "id": "massive",
  "meta": {
    "name": "Massive Market Model",
    "description": "Ingests live spot and options data from the provider and publishes near real-time options surface and raw convexity inputs into Market-Redis."
  },
  "access_points": {
    "subscribe_to": [],
    "publish_to": [
      { "bus": "system-redis", "key": "massive:heartbeat" },

      { "bus": "market-redis", "key": "massive:chain-feed" },
      { "bus": "market-redis", "key": "massive:ws:raw" }
    ]
  },
  "models": {
    "produces": [
      { "bus": "market-redis", "key": "massive:model:spot" },
      { "bus": "market-redis", "key": "massive:model:spot_trail" }
    ],
    "consumes": []
  },
  "heartbeat": {
    "interval_sec": 5,
    "ttl_sec": 15
  },
  "domain_keys": [
    "SPOT",
    "SPX",
    "NDX",
    "VIX",
    "OPTIONS",
    "GEX",
    "CONVEXITY"
  ],
  "dependencies": []
}