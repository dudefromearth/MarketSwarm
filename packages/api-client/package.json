{
  "name": "@market-swarm/api-client",
  "version": "0.1.0",
  "description": "API client for Market Swarm with offline support and platform adapters",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@market-swarm/core": "file:../core"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  },
  "keywords": [
    "api-client",
    "offline-first",
    "sync"
  ],
  "license": "UNLICENSED",
  "private": true
}
